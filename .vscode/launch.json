  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Simple Response",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "runtimeArgs": [
        "-r",
        "ts-node/register",
        "-r",
        "source-map-support/register"
      ],
      "program": "${workspaceFolder}/samples/simple-response/src/index.tsx",
      "cwd": "${workspaceFolder}/samples/simple-response",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ]
    }
  ]
}